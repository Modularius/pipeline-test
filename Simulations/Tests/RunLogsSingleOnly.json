        { "send-run-log-data" : {
            "source-name" : { "text" : "A-UByte-Value" },
            "value-type" : "uint8",
            "value" : ["52"]
        } },
        { "send-run-log-data" : {
            "source-name" : { "text" : "A-Short-Value" },
