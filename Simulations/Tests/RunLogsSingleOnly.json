{
    "voltage-transformation": {"scale": 1, "translate": 0 },
    "time-bins": 20000,
    "sample-rate": 1000000000,
    "digitiser-config": {
        "auto-digitisers": {
            "num-digitisers": { "int-env" : "NUM_DIGITISERS" },
            "num-channels-per-digitiser": { "int" : 8 }
        }
    },
    "pulses": [],
    "event-lists": [],
