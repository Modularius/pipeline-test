{
    "voltage-transformation": {"scale": 1, "translate": 0 },
    "time-bins": 20000,
    "sample-rate": 1000000000,
    "digitiser-config": {
        "auto-digitisers": {
            "num-digitisers": { "int-env" : "NUM_DIGITISERS" },
            "num-channels-per-digitiser": { "int" : 8 }
        }
    },
    "pulses": [],
    "event-lists": [],
    "schedule" : [
        { "set-timestamp": "now"},
        { "ensure-delay-ms": 0 },
        { "send-run-start" : { "name": { "text": "SelogsAndAlarmOnly" }, "instrument": { "text": "MuSR" }}},
        { "wait-ms": 200 },
        { "send-sample-env-log" : {
            "name" : { "text" : "Earth's Core" },
            "values-type" : "uint64",
            "location": "unknown",
            "time-delta": 1.0,
            "values" : ["36", "36.1", "36.8"]
        } },
        { "send-alarm" : {
            "source-name" : { "text" : "Earth's Core" },
            "severity" : "minor",
            "message": "fml"
        } },
        { "set-timestamp": { "advance-by-ms": 20}},
        { "send-alarm" : {
            "source-name" : { "text" : "Earth's Core" },
            "severity" : "major",
            "message": "wtf"
        } },
        { "set-timestamp": { "advance-by-ms": 20}},
        { "send-alarm" : {
            "source-name" : { "text" : "Earth's Core" },
            "severity" : "ok",
            "message": "fml"
        } },
        { "send-sample-env-log" : {
            "name" : { "text" : "Earth's Other Core" },
            "values-type" : "uint64",
            "location": "unknown",
            "time-delta": 1.0,
            "values" : ["36.9", "36.1", "36.8"]
        } },
        { "wait-ms": 100 },
        { "set-timestamp": { "advance-by-ms": 10000}},
        { "send-run-stop": { "name": { "text": "SelogsAndAlarmOnly" }}}
    ]
}